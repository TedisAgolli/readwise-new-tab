{
    "manifest_version": 2,
    "name": "Readwise New Tab",
    "description": "Readwise quote in your new tab",
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "tedisagolli@gmail.com",
            "strict_min_version": "42.0"
        }
    },
    "permissions": [
        "storage",
        "alarms",
        "https://readwise.io/api/*"
    ],
    "content_security_policy": "script-src 'self' 'sha256-U3p9bI/9YcclHRxNVKNCAUJm6l1ff53gO3+5gtQUEzA='"
}
